<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kaishengit.crm.mapper.AccountDeptMapper">


    <insert id="save">
        insert into t_account_dept (dept_id,account_id)values(#{deptId},#{accountId})
    </insert>
    <delete id="delect">

        delete from t_account_dept where account_id=#{id}
    </delete>
</mapper>